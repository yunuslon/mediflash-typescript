{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": [
      "es2017"
    ],
    "moduleResolution": "node",
      "baseUrl": "./" /* Base directory to resolve non-absolute module names. */,
      "paths": {
        "@component/*": [
          "./src/components/*"
        ],
        "@nav/*": [
          "./src/navigation/*"
        ],
        "@screen/*": [
          "./src/screens/*"
        ],
        "@redux/*": [
          "./src/redux/*"
        ],
        "@utils/*": [
          "./src/utils/*"
        ],
        "@asset/*": [
          "./src/assets/*"
        ]
      },
    "noEmit": true,
    "strict": true,
    "target": "esnext"
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}